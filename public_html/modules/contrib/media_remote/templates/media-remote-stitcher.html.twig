{#
/**
 * @file
 * Template implementation for the media_remote_stitcher theme hook.
 *
 * Available variables:
 * - url: (string) The full URL of the remote video.
 */
#}
<iframe style="border: solid 1px #e4edf2;"
  src="{{ url }}"
  width="220"
  height="150"
  frameborder="0"
  scrolling="no"></iframe>
